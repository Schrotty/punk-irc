//System
startUp(version) ::= "Starting punkIRC v<version>..."

//Replys
RPL_WELCOME(nick, user, host) ::= ":localhost 001 <nick> :Welcome to the Internet Relay Chat Network <nick>!<user>@<host>"

//Errors
ERR_UNKNOWNCOMMAND(client, command) ::= "<client> <command> :Unknown command."
ERR_NICKNAMEINUSE(nick, host) ::= ":<host> 433 * <nick> :Nickname is already in use."